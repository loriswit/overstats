{
  "name": "overstats-server",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^14.2.0"
  },
  "scripts": {
    "start": "node -r dotenv/config dist/server.js",
    "dev": "tsnd --no-notify --transpileOnly -r dotenv/config src/server.ts",
    "build": "tsc",
    "postinstall": "npm run build",
    "lint": "eslint --ext .ts --ignore-path .gitignore src/",
    "lint:fix": "eslint --ext .ts --ignore-path .gitignore --fix src/",
    "test": "jest --config jest.config.json",
    "test:cov": "jest --config jest.config.json --coverage"
  },
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "@typegoose/typegoose": "^7.0.0",
    "basic-auth": "^2.0.1",
    "bcrypt": "^4.0.1",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-jwt": "^3.6.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^8.0.8",
    "mongoose": "^5.9.10"
  },
  "devDependencies": {
    "@types/basic-auth": "^1.1.3",
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.0",
    "@types/koa__cors": "^3.0.1",
    "@types/mongoose": "^5.7.12",
    "@types/node": "^13.13.1",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "jest": "^26.0.1",
    "mongodb-memory-server": "^6.6.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
